

public class allOccurences {
    public static void solution(int arr[],int key,int i){
        if(i==arr.length){
            return;
        }
        if(arr[i]==key){
            System.out.print(i+" ");
        }
        solution(arr, key, i+1);

    }
    public static void main(String[] args) {
        int arr[]={3,4,3,3,3,5,6,3,7,8};
        int key=3;
        solution(arr, key, 0);
        System.out.println();
    }
    
}
