public class Solution {
    public static int[] generateFibonacciNumbers(int n) {
        // Write your code here.
        int arr[]=new int[n];
        for(int i=0;i<n;i++){
            if(i==0 || i==1){
                arr[i]=i;
            }
            else{
                arr[i]=arr[i-1]+arr[i-2];
            }
        }
        return arr;
        
            

    }
}
