import java.util.*;
public class Main {
	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int eve=0,odd=0;
		while(n!=0){
			int digit=n%10;
			if(digit%2==0){
				eve=eve+digit;
			}
			else{
				odd=odd+digit;
			}
			n=n/10;
		}
		System.out.println(eve+" "+odd);

	}
}
